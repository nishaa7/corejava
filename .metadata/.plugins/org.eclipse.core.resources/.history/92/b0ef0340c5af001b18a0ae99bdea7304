package test;

public class Loops {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
int index = 5;
while(index>0)
		
		{
	if (index!=3)
	{
	System.out.println(index);
		index--;
		}
}
int index1 = 5;
while(index1>0)
{
	System.out.println(index1);
if(index1 == 3)
{
	break;
}
index1--;
}


// do while loop

int index3=1;

do { 
	index3 = index + 1 ;
	
	System.out.println(index3);
}while(index3<11);
}
	
}